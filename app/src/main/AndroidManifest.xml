        <activity
            android:name=".Item.BorrowRequestDetails" android:parentActivityName=".MainActivity"
            android:label="@string/title_activity_borrow_request_details"
            android:theme="@style/AppTheme.NoActionBar" > </activity>
        <activity android:name=".Item.BorrowedItems" android:parentActivityName=".MainActivity"> </activity>
        <activity android:name=".Item.BorrowedItemsDetails" android:parentActivityName=".Item.BorrowedItems"> </activity>
        <activity android:name=".Item.LentItems" android:parentActivityName=".MainActivity"> </activity>
        <activity android:name=".Item.LentItemsDetails" android:parentActivityName=".Item.LentItems"> </activity>
