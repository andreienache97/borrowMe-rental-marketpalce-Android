        <activity android:name=".User.ChangeUserDetails" android:parentActivityName=".User.UserDetails"> </activity>
        <activity
            android:name=".FaqActivity" android:parentActivityName=".MainActivity"
            android:label="@string/title_activity_faq" > </activity>
        <activity
            android:name=".SupportActivity" android:parentActivityName=".Item.ItemDetails"
            android:label="User Support" > </activity>
        <activity android:name=".Item.CheckItems" />
        <activity
            android:name=".Item.InspectItem" android:parentActivityName=".LoginActivity"
            android:label="@string/title_activity_inspect_item"
            android:theme="@style/AppTheme.NoActionBar"> </activity>

        <activity android:name=".Item.InspectReportedItem"
            android:label="activity_inspect_reported_item"
            android:theme="@style/AppTheme.NoActionBar" />

        <activity android:name=".Departments.AllDepartments" android:parentActivityName=".MainActivity"> </activity>
        <activity android:name=".Top_upActivity" android:parentActivityName=".MainActivity"> </activity> //luis
        <activity android:name=".Item.ItemDetails" android:parentActivityName=".Departments.AllDepartments"> </activity>
