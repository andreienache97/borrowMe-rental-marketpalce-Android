        <activity
            android:name=".Item.ItemDetails"
            android:parentActivityName=".Departments.AllDepartments"></activity>
        <activity
            android:name=".Departments.DepartmentByName"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Departments.ItemSearch"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Item.MyItems"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Item.ReportItems" />
        <activity
            android:name=".Item.PostedItem"
            android:label="@string/title_activity_posted_item"
            android:parentActivityName=".Item.MyItems"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Item.ChangeItemDetails"
            android:label="@string/title_activity_change_item_details"
            android:parentActivityName=".Item.PostedItem"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Item.BorrowItemRequests"
            android:parentActivityName=".MainActivity"></activity>
