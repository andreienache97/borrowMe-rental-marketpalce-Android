            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Item.BorrowedItems"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Item.BorrowedItemsDetails"
            android:parentActivityName=".Item.BorrowedItems"></activity>
        <activity
            android:name=".Item.LentItems"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Item.LentItemsDetails"
            android:parentActivityName=".Item.LentItems"></activity>
        <activity
            android:name=".Chat.ChatroomsActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Chat.ChatActivity"
            android:parentActivityName=".Chat.ChatroomsActivity"></activity>
