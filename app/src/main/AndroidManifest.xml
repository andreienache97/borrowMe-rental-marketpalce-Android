<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.groupProject.borrowMe">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/start"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:parentActivityName=".LoginActivity"></activity>
        <activity
            android:name=".Admin.AdminLoginActivity"
            android:parentActivityName=".LoginActivity"></activity>
        <activity android:name=".Admin.AdminAreaActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Item.Add_itemActivity"
            android:label="Add Item"
            android:parentActivityName=".Item.ItemDetails"></activity>
        <activity
            android:name=".Helpers.Unavaliable_Date"
            android:label="Unavailable Date"
            android:parentActivityName=".Helpers.AvailableDate"></activity>
        <activity
            android:name=".Helpers.AvailableDate"
            android:label="Available Date"
            android:parentActivityName=".Item.ItemDetails"></activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name=".Submit_Activity"
            android:label="Submit"
            android:parentActivityName=".Helpers.Unavaliable_Date"></activity>
        <activity
            android:name=".User.UserDetails"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".User.ChangeUserDetails"
            android:parentActivityName=".User.UserDetails"></activity>
        <activity
            android:name=".FaqActivity"
            android:label="@string/title_activity_faq"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".SupportActivity"
            android:label="User Support"
            android:parentActivityName=".Item.ItemDetails"></activity>
        <activity android:name=".Item.CheckItems" />
        <activity android:name=".Item.FavouriteItems" />
        <activity android:name=".Item.FavouriteItemsDetails" />
        <activity
            android:name=".Item.InspectItem"
            android:label="@string/title_activity_inspect_item"
            android:parentActivityName=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Item.InspectReportedItem"
            android:label="activity_inspect_reported_item"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Departments.AllDepartments"
            android:parentActivityName=".MainActivity"></activity>
        <activity
            android:name=".Top_upActivity"
